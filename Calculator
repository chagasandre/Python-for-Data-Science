valor1 = input("Digite o primeiro número: ")
valor2 = input("Digite o segundo número: ")
operacao = input("Digite a operação: ")

resultado = []
             
if operacao == "+":
    resultado = float(valor1) + float(valor2)
elif operacao == "-":
    resultado = float(valor1) - float(valor2)
elif operacao == "*":
    resultado = float(valor1) * float(valor2)
elif operacao == "/":
    resultado = float(valor1) / float(valor2)
else:
    print("Operação não existente!")

if resultado:
    print('Resultado:', (resultado))
    
